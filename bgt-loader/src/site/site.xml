<site xsi:schemaLocation="http://maven.apache.org/SITE/2.0.0 https://maven.apache.org/xsd/site-2.0.0.xsd"
      xmlns="http://maven.apache.org/SITE/2.0.0"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      name="${this.name} ${this.version}">
  <publishDate format="dd-MMMM-yyyy"/>
  <version/>
  <skin>
    <groupId>org.apache.maven.skins</groupId>
    <artifactId>maven-fluido-skin</artifactId>
    <version>${maven-fluido-skin.version}</version>
  </skin>
  <bannerLeft name="${this.organization.name}" href="${this.organization.url}">
    <image src="images/logo.png" alt=""/>
  </bannerLeft>
  <bannerRight name="${this.name}" href="${this.url}"/>
  <body>
    <head />
    <links>
      <item name="Wiki" href="https://github.com/B3Partners/brmo/wiki" />
      <item name="Issues dashboard" href="${this.issueManagement.url}" />
    </links>
    <menu name="Informatie">
      <item name="Home" href="/index.html" />
      <item name="Developer Notes" href="/development.html" />
    </menu>
    <menu ref="parent" />
    <menu ref="modules" />
    <menu ref="reports" />
  </body>
  <poweredBy />
</site>
